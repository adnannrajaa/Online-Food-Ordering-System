
@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="~/Templates/User/assets/paper_img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>@ViewBag.Title  - Food Ordering System</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link href="~/Templates/User/bootstrap3/css/bootstrap.css" rel="stylesheet" />

    <link href="~/Templates/User/assets/css/ct-paper.css" rel="stylesheet" />
    <link href="~/Templates/User/assets/css/demo.css" rel="stylesheet" />
    <link href="~/Templates/User/assets/css/examples.css" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>

</head>
<body>
    <nav class="navbar navbar-ct-transparent navbar-fixed-top" role="navigation-demo" id="register-navbar">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" href="@Url.Action("Index","Home")" >Food Ordering System</a>
            </div>

          
        </div><!-- /.container-->
    </nav>

    <div class="wrapper">
        <div class="register-background">
            <div class="filter-black"></div>
            <div class="container">
              
                @RenderBody()
            </div>
            <div class="footer register-footer text-center">
                <h6>&copy; 2019, All Rights Reserved <i class="fa fa-heart heart"></i></h6>
            </div>
        </div>
    </div>
</body>
<script src="~/Templates/User/assets/js/jquery-1.10.2.js" type="text/javascript"></script>
<script src="~/Templates/User/assets/js/jquery-ui-1.10.4.custom.min.js" type="text/javascript"></script>
<script src="~/Templates/User/bootstrap3/js/bootstrap.js" type="text/javascript"></script>
<!--  Plugins -->
<script src="~/Templates/User/assets/js/ct-paper-checkbox.js"></script>
<script src="~/Templates/User/assets/js/ct-paper-radio.js"></script>
<script src="~/Templates/User/assets/js/bootstrap-select.js"></script>
<script src="~/Templates/User/assets/js/bootstrap-datepicker.js"></script>
<script src="~/Templates/User/assets/js/ct-paper.js"></script>

</html>
